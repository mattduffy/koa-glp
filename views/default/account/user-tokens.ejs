<main>
  <article>
    <h1>View Account Tokens</h1>
    
    <% if (view.message !== null) { %>
    <div id="messages" class="messages">
      <span><%= view.message %></span>
    </div>
    <% } %>
    <% if (view.error !== null) { %>
    <div id="errors" class="errors">
      <span><%= view.error%></span>
    </div>
    <% } %>
    <div class="account-tokens">
      <dl>
        <dt>Identity:</dt>
        <dd><%- user.jwts.token %></dd>

        <dt>Refresh:</dt>
        <dd><%- user.jwts.refresh %></dt>
      </dl>
    </div>
  </article>
</main>

